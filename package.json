{
  "name": "mobx-visualizer",
  "version": "1.1.0",
  "description": "Visualizer of MobX events using sequence diagrams",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "start": "vite --host --open",
    "clean": "rm -rf ./lib/",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "lint:scripts": "eslint ./src --ext .ts",
    "format:scripts": "prettier ./src --write",
    "lint": "eslint ./src/ --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amans199/mobx-visualizer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmed Ouda",
    "email": "ahmed.ouda1997@gmail.com",
    "url": "https://github.com/amans199"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "mobx",
    "visualizer",
    "devtools",
    "react",
    "vue",
    "svelte",
    "angular",
    "state",
    "management",
    "debugging",
    "tool"
  ],
  "bugs": {
    "url": "https://github.com/amans199/mobx-visualizer/issues"
  },
  "homepage": "https://github.com/amans199/mobx-visualizer#readme",
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^3.0.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "mermaid": "^11.4.1",
    "mobx": "^6.13.5",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.0.0"
  }
}
