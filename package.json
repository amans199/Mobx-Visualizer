{
  "name": "mobx-visualizer",
  "version": "1.0.1",
  "description": "Visualizer of MobX events using sequence diagrams",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch",
    "clean": "rm -rf ./lib/",
    "lint": "eslint ./src/ --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amans199/mobx-visualizer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Ahmed Ouda",
    "email": "ahmed.ouda1997@gmail.com",
    "url": "https://github.com/amans199"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "mobx",
    "visualizer",
    "devtools",
    "react",
    "vue",
    "svelte",
    "angular",
    "state",
    "management",
    "debugging",
    "tool"
  ],
  "bugs": {
    "url": "https://github.com/amans199/mobx-visualizer/issues"
  },
  "homepage": "https://github.com/amans199/mobx-visualizer#readme",
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.0",
    "mermaid": "^11.4.1",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^3.4.3"
  }
